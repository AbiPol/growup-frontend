{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/shell",
			"sourceRoot": "projects/shell/src",
			"prefix": "growup",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/shell/src/main.ts",
						"tsConfig": "projects/shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/shell/src/assets",
							"projects/shell/src/manifest.webmanifest",
							{
								"glob": "remoteEntry.json",
								"input": "projects/shell/src/assets",
								"output": "/"
							},
							{
								"glob": "federation.manifest.json",
								"input": "projects/shell/src/assets",
								"output": "/"
							},
							{
								"glob": "**/*",
								"input": "projects/shell/public"
							},
							{
								"glob": "**/*.{woff,woff2,ttf,eot}",
								"input": "projects/teacher/node_modules/primereact/resources/themes/lara-light-blue/fonts",
								"output": "/fonts"
							}
						],
						"styles": [
							"projects/shell/src/styles.scss"
						],
						"serviceWorker": "projects/shell/src/ngsw-config.json"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "shell:build:production"
						},
						"development": {
							"buildTarget": "shell:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		},
		"student": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/student",
			"sourceRoot": "projects/student/src",
			"prefix": "growup",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/student/src/main.ts",
						"tsConfig": "projects/student/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/student/src/assets",
							{
								"glob": "remoteEntry.json",
								"input": "projects/student/src/assets",
								"output": "/"
							},
							{
								"glob": "**/*",
								"input": "projects/student/public"
							},
							{
								"glob": "**/*",
								"input": "shared/styles",
								"output": "shared/styles"
							}
						],
						"styles": [
							"projects/student/src/styles.scss"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "student:build:production"
						},
						"development": {
							"buildTarget": "student:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		},
		"admin": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/admin",
			"sourceRoot": "projects/admin/src",
			"prefix": "growup",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/admin/src/main.ts",
						"tsConfig": "projects/admin/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/admin/src/assets",
							{
								"glob": "remoteEntry.json",
								"input": "projects/admin/src/assets",
								"output": "/"
							},
							{
								"glob": "**/*",
								"input": "projects/admin/public"
							},
							{
								"glob": "**/*",
								"input": "shared/styles",
								"output": "shared/styles"
							}
						],
						"styles": [
							"projects/admin/src/styles.scss"
						],
						"polyfills": [
							"es-module-shims"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "admin:build:production"
						},
						"development": {
							"buildTarget": "admin:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4101
					}
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		}
	}
}